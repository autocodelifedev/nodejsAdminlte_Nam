<% 
    imageHelper = (folder, imageName, alt = "Image", className = "img-responsive img-avatar", id="blah") => {

        if(imageName == null){ 
            imageName = 'no-avatar.png';
        }
        return `<img class="${className}" id="${id}" src="${folder + imageName}" alt="${alt}" style="width: 200px; height:150px"/>`;
    }
%>



